<h1>Sessions#new--Login</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<!-- display flash messages -->
<% flash.each do |k,v| %>
	<h2 id="<%= k  %>"><%= v  %></h2>
<% end %>

<%= form_for :user do |f|  %>
<%= f.text_field :email, placeholder: "Enter your email!"  %>
<%= f.password_field :password, placeholder: "password"  %>
<%= f.submit "Login"  %>

<% end %>